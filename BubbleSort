//USED BUBBLESORT TO SORT THE VOWELS
		for(int i=0;i<n;i++){
		    for(int j=0;j<n-i-1;j++){
		        if(countVowels(originial[j]) > countVowels(originial[j+1])){
		            String temp = originial[j];
		            originial[j] = originial[j+1];
		            originial[j+1] = temp;
		        }
		    }
		}
		return originial;
